{
    "compilerOptions": {
      "lib": [
          "es2017", 
          "DOM"
        ],
      "module": "commonjs",
      "target": "es2017",
      "declaration": true,
      "outDir": "./lib",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "**/__tests__/*"]
  }